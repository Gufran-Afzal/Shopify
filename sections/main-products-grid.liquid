<div class="global-block {{ section_prefix_class }} {{ section_prefix_class }}-{{ section.id }} {{ section_scheme_class }} grid-common" id="{{ section.id }}" style="--section-padding-top: {{ section.settings.section_top_padding }}px; --section-padding-bottom: {{ section.settings.section_bottom_padding }}px; --columns-count: {{ section.settings.number_of_columns_on_desktop }}; --mobile-columns-count: {{ section.settings.number_of_columns_on_mobile }};">
  <div class="container">
    <div class="{{ section_prefix_class }}_wrapper grid-common_wrapper">
      {%- unless collection.all_products_count == 0 -%}
        <header class="heading-head inner-container --align-center" style="--max-content: {{ max_header_size }}px;">
          {%- if section.settings.show_featured_image -%}
            {%- unless collection.featured_image == blank -%}
              {%- liquid
                capture image_alt
                  if collection.featured_image.alt != blank
                    echo collection.featured_image.alt
                    else
                      echo collection.title | escape
                  endif
                endcapture
              -%}
              {%- capture image_thumb_sizes -%}
                (max-width: 767px) 100px, 200px
              {%- endcapture -%}
              <div class="{{ section_prefix_class }}_featured-img">
                <img
                  srcset="{%- if collection.featured_image.width >= 100 -%}{{ collection.featured_image | image_url: width: 100 }} 100w,{%- endif -%}
                          {%- if collection.featured_image.width >= 200 -%}{{ collection.featured_image | image_url: width: 200 }} 200w{%- endif -%}"
                  sizes="{{- image_thumb_sizes -}}"
                  src="{{- collection.featured_image | image_url: width: 200 -}}"
                  alt="{{- image_alt -}}"
                  fetchpriority="high"
                  width="200"
                  height="{{- 200 | divided_by: collection.featured_image.aspect_ratio | round -}}"
                  class="object-fit object-fit--absolute"
                >
              </div>
            {%- endunless -%}
          {%- endif -%}
          {%- unless section.settings.section_caption == blank -%}
            <strong class="{{ section_prefix_class }}_caption-title --word-break {{ section.settings.section_caption_size }} {{ section.settings.section_caption_weight }}">
              {{- section.settings.section_caption -}}
            </strong>
          {%- endunless -%}
          <h1 class="mty grid-common_heading {{ section.settings.section_heading_size }} {{ section.settings.section_heading_weight }}">
            {{- collection.title -}}
          </h1>
          {% if enable_description == 'true' and section.settings.show_description == 'top' %}
            <div class="grid-common_text {{ section.settings.section_description_size }} {{ section.settings.section_description_weight }}">
              {{- collection.description -}}
            </div>
          {%- endif -%}
          <div class="grid-common_text-count mty {{ section.settings.section_description_size }} {{ section.settings.section_description_weight }}" data-render-counts="">
            {%- liquid
              if collection.products_count == collection.all_products_count
                if collection.all_products_count == 1
                  echo 'filters.count.one' | t: count: collection.products_count
                  else
                    echo 'filters.count.other' | t: count: collection.all_products_count
                endif
                else
                  echo 'filters.count.facet_by' | t: count: collection.products_count, count_all: collection.all_products_count
              endif
            -%}
          </div>
        </header>
        <div class="grid-common_area-wrap">
          {%- if section.settings.enable_filters or section.settings.enable_sort or section.settings.enable_grid_btns -%}
            <div class="grid-common_facets-bar --v-align-middle">
              {%- liquid
                if section.settings.enable_filters
                  for filter in collection.filters
                    case filter.type
                      when 'price_range'
                        if filter.min_value.value != null or filter.max_value.value != null
                          assign price_min_filter = filter.min_value.param_name
                          assign price_max_filter = filter.max_value.param_name

                          assign currencies_using_comma_decimals = 'ANG,ARS,BRL,BYN,BYR,CLF,CLP,COP,CRC,CZK,DKK,EUR,HRK,HUF,IDR,ISK,MZN,NOK,PLN,RON,RUB,SEK,TRY,UYU,VES,VND' | split: ','
                          assign uses_comma_decimals = false
                          if currencies_using_comma_decimals contains cart.currency.iso_code
                            assign uses_comma_decimals = true
                          endif

                          assign filter_min_value = filter.min_value.value | money_without_currency | replace: ',', ''
                          if uses_comma_decimals
                            assign filter_min_value = filter.min_value.value | money_without_currency | replace: '.', '' | replace: ',', '.'
                          endif

                          if filter.max_value.value
                            assign filter_max_value = filter.max_value.value | money_without_currency | replace: ',', ''
                            if uses_comma_decimals
                              assign filter_max_value = filter.max_value.value | money_without_currency | replace: '.', '' | replace: ',', '.'
                            endif
                            else
                              assign filter_max_value = filter.range_max | money_without_currency | replace: ',', ''
                              if uses_comma_decimals
                                assign filter_max_value = filter.range_max | money_without_currency | replace: '.', '' | replace: ',', '.'
                              endif
                          endif

                          assign price_min_filter = price_min_filter | append: '=' | append: filter_min_value
                          assign price_max_filter = price_max_filter | append: '=' | append: filter_max_value
                          unless base_url contains '?'
                            assign base_url = base_url | append: '?' | append: price_min_filter | append: '&' | append: price_max_filter
                            else
                              assign base_url = base_url | append: '&' | append: price_min_filter | append: '&' | append: price_max_filter
                          endunless
                        endif
                      when 'boolean', 'list'
                        for value in filter.active_values
                          assign active_filter_param = value.param_name | append: '=' | append: value.value

                          if base_url contains '?'
                            assign base_url = base_url | append: '&' | append: active_filter_param
                            else
                              assign base_url = base_url | append: '?' | append: active_filter_param
                          endif
                        endfor
                    endcase
                  endfor
                endif

                capture filter_url
                  unless base_url contains '?'
                    echo base_url | append: '?'
                    else
                      echo base_url | append: '&'
                  endunless
                endcapture
              -%}
              <facets-filter
                hidden
                data-sections='[
                  {
                    "id": "{{ section.id }}",
                    "selectors": ["[data-render-counts]", "[data-render-params]", "modal-filter-component:not(.modal--active)", "[data-render-actives]", "[data-render-grid]"]
                  },
                  {
                    "id": "modal-facets",
                    "selectors": ["[data-render-facets]"]
                  }
                ]'
              >
                <input type="hidden" value="{% unless filter_url contains '?' %}?section_id=modal-facets{% else %}section_id=modal-facets{% endunless %}" id="modal-section">
                <input type="hidden" value="{% unless filter_url contains '?' %}?sections[]=modal-facets&sections[]={{ section.id }}{% else %}&sections[]=modal-facets&sections[]={{ section.id }}{% endunless %}" id="filters-sections">
                <span data-render-params="">
                  <input type="hidden" value="{{- filter_url -}}" id="filters-params">
                </span>
                <input type="hidden" value="{{- results_url -}}" id="base-url">
              </facets-filter>
              {%- if section.settings.enable_filters -%}
                <div class="grid-common_facets-wrap grid-common_facets-bar_col grid-common_facets-bar_col--filter">
                  <modal-component-toggler>
                    <button
                      type="button"
                      data-expanded="false"
                      data-modal-ref="modal-facets"
                      aria-haspopup="dialog"
                      aria-controls="modal-facets"
                      class="button button--secondary grid-common_facet-opener --align-center --justify-center --v-align-middle --word-break"
                    >
                      <i class="button_icn-wrapper">
                        {%- render 'icons-data', width: 20, height: 20, use_icon: true, icon_reference: 'filters' -%}
                      </i>
                      <span class="grid-common_facet-opener--text">
                        {{- 'filters.drawer_label_heading' | t -}}
                      </span>
                    </button>
                  </modal-component-toggler>
                  <modal-filter-component
                    data-modal="modal-facets"
                    data-container="body"
                    data-render-selector="modal-facets"
                    data-url="{{- filter_url -}}"
                    data-selector="#facets-wrapper"
                  >
                    <div
                      class="modal modal-component --drawer modal-component--facets-drawer scheme--{{ section.settings.section_color_scheme }}"
                      role="dialog"
                      aria-modal="true"
                      aria-labelledby="modal-facets-heading"
                      aria-describedby="modal-facets"
                      tabindex="-1"
                    >
                      <div class="modal-component_wrapper scheme" id="modal-facets">
                        <div class="modal-component_body">
                          <div class="modal-component_head --align-left product-common_size-heading">
                            <modal-component-toggler>
                              <button
                                type="button"
                                data-expanded="true"
                                data-toggle-attribute="false"
                                data-modal-ref="modal-facets"
                                class="modal-component_btn-close button button--icn">
                                <span class="button_icn-wrapper">
                                  {%- render 'icons-data', width: 14, height: 14, use_icon: true, icon_reference: 'cross' -%}
                                  <span class="visually-hidden">{{- 'accessibility.close' | t -}}</span>
                                </span>
                              </button>
                            </modal-component-toggler>
                            <strong id="modal-facets-heading" class="modal-component_heading text-size--small --fw-normal mty">
                              <i class="modal-component_heading-icn">
                                {%- render 'icons-data', width: 20, height: 20, use_icon: true, icon_reference: 'filters' -%}
                              </i>
                              {{- 'filters.drawer_label_heading' | t -}}
                            </strong>
                          </div>
                          <div class="modal-component_content" id="facets-wrapper"></div>
                        </div>
                      </div>
                    </div>
                  </modal-filter-component>
                </div>
              {%- else -%}
                <div class="grid-common_facets-wrap grid-common_facets-bar_col grid-common_facets-bar_col--filter">&nbsp;</div>
              {%- endif -%}
              {%- if section.settings.enable_grid_btns -%}
                <div class="grid-common_actions-btns --justify-center --v-align-middle grid-common_facets-bar_col grid-common_facets-bar_col--actions">
                  <grid-button class="grid-common_actions-btns--tdots">
                    <span class="grid-common_grid-btn {% if section.settings.number_of_columns_on_desktop == '3' %}active{% endif %}">
                      <i class="button_icn-wrapper">
                        {%- render 'icons-data', width: 16, height: 16, use_icon: true, icon_reference: 'tdots', section_id: section.id -%}
                      </i>
                      <span class="visually-hidden">{{- 'collection.grid_buttons.3' | t -}}</span>
                    </span>
                  </grid-button>
                  <grid-button class="grid-common_actions-btns--fdots">
                    <span class="grid-common_grid-btn {% if section.settings.number_of_columns_on_desktop == '4' %}active{% endif %}">
                      <i class="button_icn-wrapper">
                        {%- render 'icons-data', width: 16, height: 16, use_icon: true, icon_reference: 'fdots', section_id: section.id -%}
                      </i>
                      <span class="visually-hidden">{{- 'collection.grid_buttons.4' | t -}}</span>
                    </span>
                  </grid-button>
                  <grid-button class="grid-common_actions-btns--sdots">
                    <span class="grid-common_grid-btn {% if section.settings.number_of_columns_on_desktop == '6' %}active{% endif %}">
                      <i class="button_icn-wrapper">
                        {%- render 'icons-data', width: 25, height: 25, use_icon: true, icon_reference: 'sdots', section_id: section.id -%}
                      </i>
                      <span class="visually-hidden">{{- 'collection.grid_buttons.6' | t -}}</span>
                    </span>
                  </grid-button>
                </div>
              {%- endif -%}
              {%- if section.settings.enable_sort -%}
                <div class="grid-common_sort-wrap dropdown-component grid-common_facets-bar_col grid-common_facets-bar_col--sort">
                  <strong id="facets-sort-select-heading" class="visually-hidden" role="heading" aria-level="2">
                    {{- 'filters.sort.label_heading' | t -}}
                  </strong>
                  <div class="dropdown-sort_wrap">
                    <sort-dropdown
                      class="dropdown-sort"
                    >
                      <button
                        type="button"
                        class="dropdown-component_opener grid-common_opener dropdown-sort_opener --align-left --word-break"
                        aria-expanded="false"
                        aria-controls="facets-sort-select"
                        aria-describedby="facets-sort-select-heading"
                      >
                        <span data-current-value="" class="dropdown-sort_title">
                          {%- liquid
                            for option in collection.sort_options
                              if option.value == sort_by
                                echo option.name | escape
                              endif
                            endfor
                          -%}
                        </span>
                        <i class="dropdown-componenet_icn-caret">
                          {%- render 'icons-data', width: 9, height: 5, use_icon: true, icon_reference: 'chevron-down', section_id: section.id -%}
                        </i>
                      </button>
                      <div class="dropdown-component_wrapper dropdown-sort_wrapper">
                        <ul id="facets-sort-select" class="list-unstyled dropdown-sort_items">
                          {%- for option in collection.sort_options -%}
                            <li>
                              <button
                                data-btn-sort=""
                                type="button"
                                class="dropdown-component_btn-submit --word-break --align-left facet-sort_btn-submit {% if option.value == sort_by %}active{% endif %}"
                                data-value="{{- option.value | escape -}}"
                                data-text="{{- option.name | escape -}}"
                              >
                                {{- option.name | escape -}}
                                {%- if option.value == sort_by -%}
                                  <span class="visually-hidden">{{- 'filters.sort.option_selected' | t -}}</span>
                                {%- endif -%}
                              </button>
                            </li>
                          {%- endfor -%}
                        </ul>
                      </div>
                      <input type="hidden" name="sort_by" value="{{- sort_value -}}">
                    </sort-dropdown>
                  </div>
                </div>
              {%- else -%}
                <div class="grid-common_sort-wrap dropdown-component grid-common_facets-bar_col grid-common_facets-bar_col--sort">&nbsp;</div>
              {%- endif -%}
            </div>
            {%- if section.settings.enable_filters -%}
              {%- liquid
                assign active_filters_size = 0

                for filter in collection.filters
                  for value in filter.active_values
                    assign active_filters_size = active_filters_size | plus: 1
                  endfor

                  if filter.type == 'price_range'
                    if filter.min_value.value != null or filter.max_value.value != null
                      assign active_filters_size = active_filters_size | plus: 1
                    endif
                  endif
                endfor
              -%}
              <div class="parent-node" data-render-actives="">
                <div class="grid-common_active-facets mty {% unless active_filters_size > 0 %}hidden{% endunless %}">
                  {%- for filter in collection.filters -%}
                    {%- for value in filter.active_values -%}
                      <facet-remove-button
                        data-name="{{- filter.param_name -}}"
                      >
                        <button type="button" class="facet-remove-btn button button--primary --v-align-middle">
                          {{- value.label | escape -}}
                          <i class="button_icn-wrapper">
                            {%- render 'icons-data', width: 8, height: 8, use_icon: true, icon_reference: 'cross' -%}
                          </i>
                        </button>
                      </facet-remove-button>
                    {%- endfor -%}
                    {% if filter.type == 'price_range' %}
                      {%- if filter.min_value.value != null or filter.max_value.value != null -%}
                        <facet-remove-button
                          data-name="{{- filter.param_name -}}"
                        >
                          <button type="button" class="facet-remove-btn button button--primary --v-align-middle">
                            {%- liquid
                              if filter.min_value.value
                                echo filter.min_value.value | money
                                else
                                  echo 0 | money
                              endif
                              echo ' - '
                              if filter.max_value.value
                                echo filter.max_value.value | money
                                else
                                  echo filter.range_max | money
                              endif
                            -%}
                            <i class="button_icn-wrapper">
                              {%- render 'icons-data', width: 8, height: 8, use_icon: true, icon_reference: 'cross' -%}
                            </i>
                          </button>
                        </facet-remove-button>
                      {%- endif -%}
                    {%- endif -%}
                  {%- endfor -%}
                  <facet-remove-button
                    data-name="all"
                  >
                    <button type="button" class="facets-remove-all button button--link --hover-underline">
                      {{- 'filters.clear_all' | t -}}
                    </button>
                  </facet-remove-button>
                </div>
              </div>
            {%- endif -%}
          {%- endif -%}
          {%- unless collection.products.size == 0 -%}
            <div class="grid-common_cols-wrap grid grid--no-gutters {{ section_prefix_class }}_cols-wrap" data-render-grid="">
              {%- paginate collection.products by max_products_to_show -%}
                {%- for product in collection.products -%}
                  <div class="grid_col" data-render-columns="">
                    {%- liquid
                      render 'product-column', product: product, heading_tag: 'h2', columns_count: section.settings.number_of_columns_on_desktop, media_size: section.settings.product_media_size, enable_atc: section.settings.enable_atc, show_swatches: section.settings.show_swatches, show_price: section.settings.show_price, show_vendor: section.settings.show_vendor, show_stock_tag: section.settings.show_stock_tag, show_sale_tag: section.settings.show_sale_tag, heading_size: section.settings.product_heading_size, heading_weight: section.settings.product_heading_weight, btns_position: section.settings.btns_position, show_quick_view: section.settings.enable_quick_view, loading_attr: 'lazy', section_id: section.id
                    -%}
                  </div>
                {%- endfor -%}
                {%- liquid
                  case section.settings.paging_type
                    when 'click_load'
                      render 'load-more', paginate: paginate, infinite: false, section_id: section.id
                    when 'load_more'
                      render 'load-more', paginate: paginate, infinite: true, section_id: section.id
                    else
                      render 'pagination', paginate: paginate, prefix_class: section_prefix_class, section_id: section.id
                  endcase
                -%}
              {%- endpaginate -%}
            </div>
            {% if enable_description == 'true' and section.settings.show_description == 'bottom' %}
              <div class="grid-common_text {{ section.settings.section_description_size }} {{ section.settings.section_description_weight }}">
                {{- collection.description -}}
              </div>
            {%- endif -%}
            {%- else -%}
              <div class="grid-common_empty --align-center" data-render-grid="">
                <i class="grid-common_empty-icn">
                  {%- render 'icons-data', width: 44, height: 44, use_icon: true, icon_reference: 'empty-folder' -%}
                </i>
                <strong class="grid-common_heading mty {{ section.settings.section_heading_size }} {{ section.settings.section_heading_weight }}">
                  {{- 'collection.empty_heading' | t -}}
                </strong>
                <div class="grid-common_text grid-common_text--empty {{ section.settings.section_description_size }} {{ section.settings.section_description_weight }}">
                  <p>{{- 'filters.use_fewer_filters' | t -}}</p>
                  <facet-remove-button
                    data-name="all"
                  >
                    <button type="button" class="facets-remove-all button button--primary">
                      {{- 'filters.clear_all' | t -}}
                    </button>
                  </facet-remove-button>
                </div>
              </div>
          {%- endunless -%}
        </div>
        {%- else -%}
          <div class="grid-common_empty --align-center">
            <i class="grid-common_empty-icn">
              {%- render 'icons-data', width: 44, height: 44, use_icon: true, icon_reference: 'empty-folder' -%}
            </i>
            <strong class="grid-common_heading mty {{ section.settings.section_heading_size }} {{ section.settings.section_heading_weight }}">
              {{- 'collection.empty_heading' | t -}}
            </strong>
            <div class="grid-common_text grid-common_text--empty {{ section.settings.section_description_size }} {{ section.settings.section_description_weight }}">
              <p>{{- 'collection.empty_description' | t -}}</p>
            </div>
            <footer class="grid-common_foot">
              <a href="{{- routes.collections_url -}}" class="button button--primary">
                {{- 'collection.empty_btn_label' | t -}}
              </a>
            </footer>
          </div>
      {%- endunless -%}
    </div>
  </div>
</div>
{% schema %}
