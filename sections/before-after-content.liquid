{%- liquid
  assign section_prefix_class = 'before-after-content'
  assign section_scheme_class = section.settings.section_color_scheme | prepend: 'scheme scheme--'

  capture max_header_size
    echo settings.page_width | times: section.settings.max_header_size | divided_by: 100
  endcapture

  capture cols_order_class
    if section.settings.section_media_order == 'row_reverse'
      echo '--row-reverse'
    endif
  endcapture

  capture make_image_full_width
    if section.settings.make_image_full_width
      echo '--full-width'
      else
        echo '--no-width'
    endif
  endcapture

  capture content_unavailable
    if section.settings.heading != blank or section.settings.section_description != blank or section.settings.view_more_label != blank
      echo '--image-contain'
      else
        echo '--no-contain'
    endif
  endcapture

  capture bf_height
    case section.settings.bf_height
      when 'small'
        echo '--small-height'
      when 'medium'
        echo '--medium-height'
      when 'large'
        echo '--large-height'
    endcase
  endcapture

  capture loading_attr
    unless section.index != 1
      echo 'eager'
      else
        echo 'lazy'
    endunless
  endcapture
-%}
{%- style -%}
  .before-after .heading-head{
    margin-bottom: 40px;
  }

  .bf-outer-wrap,
  .bf-caption{
    padding-left: 26px;
    padding-right: 26px;
    margin-bottom: 26px;
  }

  .before-after_description{
    --mt-top: 24px;
  }

  .bf-wrapper{
    position: relative;
    display: block;
  }

  .bf-wrapper,
  .bf-wrapper_pointer input[type="range"],
  .bf-wrapper_image--before,
  .bf-wrapper_image--after,
  .bf-wrapper_pointer,
  .bf-wrapper_point{
    width: 100%;
  }

  .bf-wrapper, .bf-wrapper_image, .bf-wrapper_image--after, .bf-wrapper_pointer{
    --height-factor: 0.5;
  }

  .bf-wrapper_image--after{
    clip-path: inset(0% 0% 0% var(--after-placement, 50%));
    overflow: hidden;
    z-index: 1;
  }

  .bf-wrapper_image--after,
  .bf-wrapper_pointer,
  .bf-wrapper_image--before,
  .bf-wrapper_point::after,
  .bf-wrapper_point{
    height: 100%;
  }

  .bf-wrapper_image--before,
  .bf-wrapper_image--after,
  .bf-wrapper_pointer,
  .bf-wrapper_point,
  .bf-wrapper_point::before,
  .bf-wrapper_point::after{
    left: 0;
    top: 0;
  }

  .bf-wrapper_image--before,
  .bf-wrapper_image--after,
  .bf-wrapper_pointer,
  .bf-wrapper_point,
  .bf-wrapper_point::before,
  .bf-wrapper_point::after{
    position: absolute;
  }

  .bf-wrapper_pointer,
  .bf-caption{
    display: flex;
  }

  .bf-caption_col{
    width: 50%;
  }

  .bf-caption_col--first{
    padding-right: 8px;
  }

  .bf-caption_subheading{
    --heading-size-factor: 0.714287;
  }

  .bf-wrapper_pointer input[type="range"]{
    --focus-outline-offset: 1px;
    -webkit-appearance: none;
    background: transparent;
    cursor: grab;
    margin: 0;
    z-index: 2;
  }

  .bf-wrapper_pointer input[type="range"]::-webkit-slider-thumb{
    opacity: 0;
  }

  .bf-wrapper_pointer input[type="range"]::-moz-range-thumb{
    opacity: 0;
  }

  .bf-wrapper_pointer input[type="range"]::-webkit-slider-thumb:focus,
  .bf-wrapper_pointer input[type="range"]::-webkit-slider-thumb:focus-visible{
    outline: none;
  }

  .bf-wrapper_pointer input[type="range"]:focus::-webkit-slider-thumb,
  .bf-wrapper_pointer input[type="range"]:focus-visible::-webkit-slider-thumb{
    opacity: 0;
  }

  .bf-wrapper_error-text{
    color: var(--section-error-color);
  }

  .bf-wrapper_point::before,
  .bf-wrapper_point::after{
    content: '';
    pointer-events: none;
  }

  .bf-wrapper_point::before{
    height: 40px;
    width: 40px;
    top: 50%;
    left: var(--after-placement);
    transform: translate(-50%, -50%);
    border-radius: 100%;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-position: 50% 50%;
    background-size: 50% auto;
    background-color: #fff;
    box-shadow: 1px 1px 1px #000, 0 0 1px #0d0d0d;
    content: '';
    z-index: 2;
  }

  .bf-wrapper_point::after{
    border-right: 1px solid var(--section-background-color);
    width: calc(var(--after-placement, 50%) + 1px);
    z-index: 1;
  }

  .bf-wrapper_image:nth-child(1), .bf-wrapper_image:nth-child(2){
    border-radius: var(--section-radius);
  }

  .before-after-content .--no-contain .bf-wrapper_image:nth-child(1), 
  .before-after-content .--no-contain .bf-wrapper_image:nth-child(2),
  .before-after-content_wrapper.--full-width .bf-wrapper_image:nth-child(1),
  .before-after-content_wrapper.--full-width .bf-wrapper_image:nth-child(2){
    border-radius: 0;
  }

  .bf-wrapper_image svg, .bf-wrapper_image img{
    border-radius: inherit;
  }

  .bf-caption.--row-reverse{
    margin-left: auto;
  }

  .before-after-content{
    --q-align-value: var(--mobile-q-align);
  }

  .bf-outer-wrap,
  .bf-wrapper{
    height: 100%;
  }

  .before-after-content_col-description,
  .before-after-content_wrapper{
    display: flex;
  }

  .before-after-content_wrapper{
    gap: 20px 10px;
    flex-direction: column;
  }

  .before-after-content .video-card .video-card_media{
    min-height: 150px;
  }

  .before-after-content .video-card .video-card_media,
  .before-after-content_image-wrap{
    min-height: 150px;
  }

  .before-after-content_image-wrap{
    border-radius: var(--section-radius);
    overflow: hidden;
  }

  .before-after-content_image-wrap img{
    border-radius: inherit;
  }

  strong.h5.before-after-content_caption-title,
  strong.h4.before-after-content_caption-title,
  strong.h6.before-after-content_caption-title{
    display: block;
    margin-bottom: 17px;
    margin-top: 0;
  }

  .before-after-content_caption {
    margin-bottom: 30px;
    margin-top: 4px;
  }

  .before-after-content_foot{
    margin-top: 45px;
  }

  .before-after-content_description ul{
    padding-left: 0;
  }

  .before-after-content_col-media{
    flex-shrink: 0;
  }

  .before-after-content_col-description{
    flex-grow: 1;
    padding-left: 26px;
    padding-right: 26px;
  }

  .before-after-content_wrapper.--full-width{
    flex-direction: column-reverse;
  }

  .before-after-content_wrapper.--full-width .before-after-content_col-media,
  .before-after-content_wrapper.--full-width .before-after-content_col-description,
  .before-after-content_col-media.--no-contain,
  .before-after-content_col-description,
  .before-after-content_col-media,
  .bf-caption.--full-width{
    width: 100%;
  }

  .before-after-content_wrapper.--full-width .before-after-content_col-description{
    padding-left: 26px;
    padding-right: 26px;
  }

  .before-after-content_wrapper.--full-width.--row-reverse{
    flex-direction: column-reverse;
  }

  @media (min-width: 576px){
    .bf-wrapper,
    .bf-wrapper_image,
    .bf-wrapper_image--after,
    .bf-wrapper_pointer{
      --height-factor: 0.7;
    }
  }

  @media (min-width: 768px){
    .bf-wrapper,
    .bf-wrapper_image,
    .bf-wrapper_image--after,
    .bf-wrapper_pointer{
      --height-factor: 0.85;
    }

    .bf-outer-wrap,
    .bf-caption{
      margin-bottom: 0;
    }

    .bf-caption{
      padding-top: 20px;
    }

    .before-after-content_wrapper.--row-reverse{
      flex-direction: row-reverse;
    }

    .before-after-content{
      --q-align-value: var(--desktop-q-align);
    }

    .before-after-content_wrapper{
      display: flex;
      column-gap: 10px;
      flex-direction: unset;
    }

    .before-after-content_col-media,
    .bf-caption{
      width: 48%;
    }

    .before-after-content_col-description{
      flex-grow: 1;
      width: unset;
      padding-left: 0;
      padding-right: 0;
    }

    .--hidden-desktop{
      display: none;
    }
  }

  @media (min-width: 992px){
    .bf-wrapper,
    .bf-wrapper_image,
    .bf-wrapper_image--after,
    .bf-wrapper_pointer{
      --height-factor: 1;
    }
  }

  @media (min-width: 1200px){
    .bf-wrapper_point::before{
      height: 60px;
      width: 60px;
    }

    .before-after-content_wrapper{
      column-gap: 50px;
    }

    .before-after-content_caption {
      margin-bottom: 50px;
    }
  }

  @media (max-width: 767px){
    .--hidden-mobile{
      display: none;
    }
  }
{%- endstyle -%}
<div class="global-block {{ section_prefix_class }} {{ section_prefix_class }}-{{ section.id }} {{ section_scheme_class }} {{ section_prefix_class }}--bdr-bottom {{ section_prefix_class }}--bdr-top --content-{{ section.settings.content_alignment }}" style="--section-padding-top: {{ section.settings.section_top_padding }}px; --section-padding-bottom: {{ section.settings.section_bottom_padding }}px; --mobile-q-align: {{ section.settings.section_mobile_text_alignment }}; --desktop-q-align: {{ section.settings.section_text_alignment }};">
  <div class="container">
    <div class="{{ section_prefix_class }}_wrapper {{ make_image_full_width }} {{ cols_order_class }}">
      <div class="{{ section_prefix_class }}_col {{ section_prefix_class }}_col-media {{ content_unavailable }}">
        {%- render 'before-after', height: bf_height, loading_attr: loading_attr -%}
        {%- if section.blocks.size == 2 -%}
          <div class="bf-caption --align-left {{ make_image_full_width }} {{ cols_order_class }} --hidden-desktop">
            {%- for block in section.blocks -%}
              {%- case block.type -%}
                {%- when 'before' -%}
                  <div class="bf-caption_col bf-caption_col--first" {{ block.shopify_attributes }}>
                    {%- if block.settings.heading != blank -%}
                      <h3 class="mty bf-caption_heading {{ block.settings.heading_size }} {{ block.settings.heading_weight }}">
                        {{- block.settings.heading -}}
                      </h3>
                    {%- endif -%}
                    {%- if block.settings.sub_heading != blank -%}
                      <h4 class="mty bf-caption_subheading {{ block.settings.sub_heading_weight }}">
                        {{- block.settings.sub_heading -}}
                      </h4>
                    {%- endif -%}
                  </div>
              {%- endcase -%}
            {%- endfor -%}
            {%- for block in section.blocks -%}
              {%- case block.type -%}
                {%- when 'after' -%}
                  <div class="bf-caption_col bf-caption_col--second" {{ block.shopify_attributes }}>
                    {%- if block.settings.heading != blank -%}
                      <h3 class="mty bf-caption_heading {{ block.settings.heading_size }} {{ block.settings.heading_weight }}">
                        {{- block.settings.heading -}}
                      </h3>
                    {%- endif -%}
                    {%- if block.settings.sub_heading != blank -%}
                      <h4 class="mty bf-caption_subheading {{ block.settings.sub_heading_weight }}">
                        {{- block.settings.sub_heading -}}
                      </h4>
                    {%- endif -%}
                  </div>
              {%- endcase -%}
            {%- endfor -%}
          </div>
        {%- endif -%}
      </div>
      {%- if section.settings.heading != blank or section.settings.section_description != blank or section.settings.view_more_label != blank -%}
        <div class="{{ section_prefix_class }}_col {{ section_prefix_class }}_col-description --q-align">
          {%- if section.settings.section_heading != blank or section.settings.section_description != blank -%}
            <header class="heading-head inner-container --content_child --word-break --q-align" style="--max-content: {{ max_header_size }}px;">
              {%- if section.settings.section_heading != blank -%}
                <h2 class="{{ section_prefix_class }}_heading --h-anchor-none {{ section.settings.section_heading_size }} {{ section.settings.section_heading_weight }} mty">
                  {{- section.settings.section_heading -}}
                </h2>
              {%- endif -%}
              {%- if section.settings.section_caption != blank -%}
                <div class="{{ section_prefix_class }}_caption --word-break {{ section.settings.caption_text_size }} {{ section.settings.section_caption_weight }} mty">
                  {{- section.settings.section_caption -}}
                </div>
              {%- endif -%}
              {%- if section.settings.section_description != blank -%}
                <div class="{{ section_prefix_class }}_description --word-break {{ section.settings.description_text_size }} {{ section.settings.section_description_weight }} mty">
                  {{- section.settings.section_description -}}
                </div>
              {%- endif -%}
              {%- if section.settings.view_more_label != blank -%}
                <div class="{{ section_prefix_class }}_foot">
                  <a
                    {% if section.settings.view_more_link != blank -%}href="{{ section.settings.view_more_link }}" {% if section.settings.open_new_window %}target="_blank"{% endif %} {% else %} aria-disabled="true" {% endif %}
                    class="button button--{{ section.settings.view_more_type }}"
                  >
                    {{- section.settings.view_more_label -}}
                  </a>
                </div>
              {%- endif -%}
            </header>
          {%- endif -%}
        </div>
      {% endif %}
    </div>
    {%- if section.blocks.size == 2 -%}
      <div class="bf-caption --align-left {{ make_image_full_width }} {{ cols_order_class }} --hidden-mobile">
        {%- for block in section.blocks -%}
          {%- case block.type -%}
            {%- when 'before' -%}
              <div class="bf-caption_col bf-caption_col--first" {{ block.shopify_attributes }}>
                {%- if block.settings.heading != blank -%}
                  <h3 class="mty bf-caption_heading {{ block.settings.heading_size }} {{ block.settings.heading_weight }}">
                    {{- block.settings.heading -}}
                  </h3>
                {%- endif -%}
                {%- if block.settings.sub_heading != blank -%}
                  <h4 class="mty bf-caption_subheading {{ block.settings.sub_heading_weight }}">
                    {{- block.settings.sub_heading -}}
                  </h4>
                {%- endif -%}
              </div>
          {%- endcase -%}
        {%- endfor -%}
        {%- for block in section.blocks -%}
          {%- case block.type -%}
            {%- when 'after' -%}
              <div class="bf-caption_col bf-caption_col--second" {{ block.shopify_attributes }}>
                {%- if block.settings.heading != blank -%}
                  <h3 class="mty bf-caption_heading {{ block.settings.heading_size }} {{ block.settings.heading_weight }}">
                    {{- block.settings.heading -}}
                  </h3>
                {%- endif -%}
                {%- if block.settings.sub_heading != blank -%}
                  <h4 class="mty bf-caption_subheading {{ block.settings.sub_heading_weight }}">
                    {{- block.settings.sub_heading -}}
                  </h4>
                {%- endif -%}
              </div>
          {%- endcase -%}
        {%- endfor -%}
      </div>
    {%- endif -%}
  </div>
</div>
