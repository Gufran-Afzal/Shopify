{%- liquid
  assign section_prefix_class = 'featured-product'
  assign section_scheme_class = section.settings.section_color_scheme | prepend: 'scheme scheme--'
  assign product = section.settings.featured_product
  assign selected_variant = product.selected_or_first_available_variant
  assign current_values = selected_variant.options
  assign first_3d_model = product.media | where: 'media_type', 'model' | first
  if product.selected_or_first_available_variant.featured_media
    assign seo_media = product.selected_or_first_available_variant.featured_media
    else
      assign seo_media = product.featured_media
  endif

  assign product_json_id = 'ProductCardJson-' | append: product.id | append: '-' | append: section.id
  assign form_id = 'featured-product-atc-form' | append: product.id | append: '-' | append: section.id
  assign product_form_installment_id = 'main-form-installment' | append: product.id | append: '-' | append: section.id
  assign price_render_attr = 'data-render-price'
  assign sku_render_attr = 'data-render-sku'
  assign stock_render_attr = 'data-render-stock'
  assign qty_render_attr = 'data-render-qty'
  assign stockbar_render_attr = 'data-render-stockbar'
  assign pickup_render_attr = 'data-render-pickup'
  assign share_render_attr = 'data-render-share'
  assign media_slider_attr = 'data-change-slide-' | append: section.id
  assign media_static_attr = 'data-change-media' | append: section.id
  assign error_render_attr = 'data-render-main-error' | append: section.id

  capture enable_deferred_css
    if first_3d_model
      echo 'true'
      else
        for media in product.media
          if media.media_type == 'external_video' or media.media_type == 'video'
            echo 'true'
            break
          endif
        endfor
    endif
  endcapture

  capture enable_reviews
    for block in section.blocks
      case block.type
        when 'review'
          echo 'true'
          break
      endcase
    endfor
  endcapture

  capture enable_share
    for block in section.blocks
      case block.type
        when 'action_buttons'
          if block.settings.enable_share
            echo 'true'
          endif
          break
      endcase
    endfor
  endcapture

  capture enable_collapsibles
    for block in section.blocks
      case block.type
        when 'collapsible_row'
          echo 'true'
          break
      endcase
    endfor
  endcapture

  capture enable_pickup
    for block in section.blocks
      case block.type
        when 'pickup_availability'
          assign pick_up_availabilities = product.selected_or_first_available_variant.store_availabilities | where: 'pick_up_enabled', true
          if pick_up_availabilities.size > 0
            echo 'true'
            break
          endif
      endcase
    endfor
  endcapture

  assign variant_styles = 'pills'
  capture enable_variants
    for block in section.blocks
      case block.type
        when 'variants_chart'
          if block.settings.variant_style == 'dropdown'
            assign variant_styles = 'dropdown'
          endif
          echo 'true'
          break
      endcase
    endfor
  endcapture

  capture product_title
    if product.title != blank
      echo product.title
      else
        echo 'product.title' | t
    endif
  endcapture

  capture enable_row_reviews
    if enable_reviews == 'true' and section.settings.reviews_position == 'vertical'
      echo 'true'
    endif
  endcapture

  capture enable_aks_popup
    for block in section.blocks
      case block.type
        when 'action_buttons'
          if block.settings.enable_aks_popup
            echo 'true'
          endif
          break
      endcase
    endfor
  endcapture

  capture product_clean_url
    if product.url contains '?'
      assign parts = product.url | split: '?'
      echo parts[0]
      else
        echo product.url
    endif
  endcapture

  capture loading_attr
    unless section.index != 1
      echo 'eager'
      else
        echo 'lazy'
    endunless
  endcapture
-%}
{%- style -%}
  .form__message,
  .error-email {
    display: flex;
  }

  .form-group{
    position: relative;
  }

  .form__message,
  .error-email {
    align-items: center;
    gap: 10px;
  }

  .form__message {
    font-weight: 400;
    font-size: 1.8rem;
    line-height: 2.8rem;
  }

  .newsletter-form_results-wrap,
  .form__message svg,
  .errors-list_link {
    color: var(--section-error-color);
  }

  .success-message .image,
  .errors-list {
    margin-bottom: 10px;
  }

  .errors-list_link {
    text-decoration: none;
  }

  .error-email {
    position: absolute;
    right: 0;
  }

  .error-email,
  .contact__field-error {
    font-size: 1.4rem;
    line-height: 2.1rem;
  }

  .success-message {
    text-align: center;
    margin-top: 39px;
    font-size: 2rem;
    line-height: 3.3rem;
    color: var(--section-success-color);
  }

  .success-message p {
    margin: 0;
  }

  .success-message_icn {
    display: block;
    margin-bottom: 14px;
  }

  .form-group textarea,
  .error-email{
    margin-top: 10px;
  }

  .form-wrapper_btn-wrap{
    margin-top: 19px;
  }

  .form-wrapper_button {
    width: 100%;
    --button-padding-y: 1.1rem;
    --button-padding-x: 1.6rem;
    --button-font-size: 1.4rem;
    --button-line-height: 2rem;
    --button-font-family: var(--body-font-family);
  }

  .modal-component--question-ask .form-group--name,
  .modal-component--question-ask .form-group--email{
    margin-bottom: 10px;
  }

  .modal-component--question-ask .error-email{
    position: static;
    justify-content: end;
  }

  .featured-product{
    overflow: visible;
  }

  .featured-product_wrapper,
  .featured-product_col--description:only-child{
    width: 100%;
  }

  .modal-component--question-ask .modal-component_head{
    margin-bottom: 20px;
  }

  .contact-form{
    --q-align-value: var(--mobile-q-align);
  }

  .contact-form .container-fluid{
    --cp-spacer-left: 0;
    --cp-spacer-right: 0;
  }

  .locator-row--iframe {
    position: relative;
  }

  .contact-form .form-group textarea {
    resize: none;
    --input-height: 208px;
  }

  .contact-form .form-group input{
    --input-height: 4.6rem;
  }

  .contact-form_wrapper,
  .contact-form .two-form-group {
    display: flex;
  }

  .contact-form .two-form-group{
    gap: 19px;
  }

  .contact-form_form-wrapper .form-group--email,
  .contact-form_form-wrapper .form-group--name{
    margin-bottom: 30px;
    width: 50%;
  }

  .contact-form_wrapper {
    flex-wrap: wrap;
    gap: 18px;
    margin-top: 20px;
  }

  .contact-form_col-form,
  .locator-row--iframe iframe,
  .locator-row--iframe svg{
    width: 100%;
  }

  .contact-form_col-info {
    flex-grow: 1;
    flex-basis: 0;
  }

  .contact-form .form-wrapper_heading {
    --mt-bottom: 10px;
  }

  .contact-form_col-form .heading-head{
    margin-bottom: 25px;
  }

  .form-wrapper_main-heading{
    --mt-bottom: 28px;
  }

  .locator-row--iframe{
    line-height: 0;
    height: 100%;
  }

  .locator-row--iframe iframe,
  .locator-row--iframe svg{
    margin: 0;
    height: 300px;
  }

  .locator-row_col--iframe{
    margin-top: 37px;
  }

  .share-button-wrap{
    margin-left: auto;
  }

  .share-button-wrap,
  .dropdown-component--share{
    position: relative;
  }

  .field__label,
  .share-button__copy,
  .share-button__close,
  .dropdown-component--share .field__label,
  .dropdown-component--share .share-button__message{
    position: absolute;
  }

  .dropdown-component_share-wrapper{
    width: 273px;
    right: -64px;
    left: auto;
    top: auto;
    bottom: 100%;
    transform: none;
    margin-bottom: 10px;
    padding: 40px 60px 10px 10px;
  }

  .--share-native .dropdown-component_share-wrapper{
    display: none;
  }

  .dropdown-component--share .field__label,
  .share-button__close{
    top: 5px;
  }

  .field__label{
    left: 23px;
  }

  .share-button__copy{
    right: 10px;
    bottom: 10px;
  }

  .share-button-wrap .form-group{
    width: calc(100% - 50px);
  }

  .share-button__close{
    right: 7px;
  }

  .dropdown-component--share .field__label,
  .share-button__message{
    top: -32px;
    left: 23px;
    z-index: 1;
  }

  .share-button__copy.button--icn{
    --button-padding-y: 1.4rem;
    --button-padding-x: 1.3rem;
  }

  .share-button__close.button--icn,
  .share-button__copy.button--icn{
    --btn-icn-bdr: transparent;
  }

  .share-button__close.button--icn{
    --button-padding-y: 0.5rem;
    --button-padding-x: 0.5rem;
  }

  .collapsible-row_opener{
    display: flex;
    padding-top: 11px;
    padding-bottom: 11px;
    border-bottom: 1px solid var(--border-color);
    cursor: pointer;
  }

  .collapsible-row_opener-icn{
    margin-left: auto;
  }

  [open] summary > .collapsible-row_opener--icn-open,
  .collapsible-row_opener--icn-close{
    display: none;
  }

  [open] summary > .collapsible-row_opener--icn-close{
    display: inline;
  }

  .product-pickup_wrapper{
    padding: 23px 28px;
    border-radius: calc(var(--section-radius) / 2);
  }

  .product-pickup_text-wrap{
    display: flex;
    gap: 20px;
    font-style: normal;
  }

  .product-pickup_icn{
    flex-shrink: 0;
    width: 30px;
    line-height: 0;
  }

  .product-pickup_opener-link.button--link{
    --button-padding-y: 0.15rem;
    --button-padding-x: 0;
    --button-font-weight: 500;
    --button-text: var(--section-widget-links-color);
    --button-hover-text: var(--section-widget-active-hover-color);
    text-decoration: underline;
  }

  .product-pickup_opener-link.button--link:hover{
    text-decoration: none;
  }

  .product-pickup_text-wrap + .product-pickup_text-wrap,
  .product-pickup_wrapper + .product-pickup_wrapper,
  .product-pickup_cross-icn{
    margin-top: 12px;
  }

  .modal-component--pickup-drawer .modal-component_wrapper{
    max-width: 470px;
  }

  .modal-component--pickup-drawer .modal-component_body{
    min-height: 100%;
    padding: 24px;
  }

  .product-pickup_heading,
  .product-pickup_subheading{
    display: block;
  }

  .product-pickup_options-list,
  .product-pickup_subheading{
    margin-top: 5px;
  }

  .product-pickup_showcase-list{
    margin-top: 10px;
  }

  @media (min-width: 576px){
    .dropdown-component_share-wrapper{
      width: 350px;
      right: 0;
    }
  }

  @media (min-width: 768px){
    .contact-form{
      --q-align-value: var(--desktop-q-align);
    }

    .contact-form{
      overflow: visible;
    }

    .contact-form_wrapper {
      gap: 20px;
      margin-top: 50px;
    }

    .contact-form_col-form {
      width: 53%;
    }

    .contact-form--form-center .contact-form_col-form {
      margin-left: auto;
      margin-right: auto;
      width: 100%;
      max-width: 703px;
    }

    .locator-row--iframe iframe,
    .locator-row--iframe svg{
      height: 400px;
    }

    .dropdown-component_share-wrapper{
      width: 430px;
    }
  }

  @media (min-width: 992px){
    .featured-product_wrapper{
      display: flex;
      gap: 30px;
    }
    
    .featured-product_col--media{
      width: calc(60% - 15px);
    }
    
    .featured-product_col--description{
      width: calc(40% - 15px);
    }
  }

  @media (min-width:1200px) {
    .contact-form_wrapper {
      gap: 54px;
      margin-top: 72px;
    }

    .locator-row--iframe iframe,
    .locator-row--iframe svg{
      height: 686px;
    }
  }
{%- endstyle -%}
