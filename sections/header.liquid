                            {%- form 'localization', id: resid, class: 'localization-form' -%}
                              {%- render 'country-localization', localPosition: section.id, id: resid, resp: "-mob", section_id: section.id -%}
                            {%- endform -%}
                          </div>
                        {%- endif -%}
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </modal-component>
          </div>
        </div>
      </nav>
    </div>
  {%- unless section.settings.enable_sticky == 'none' -%}
    </sticky-block>
  {%- endunless -%}
</div>

