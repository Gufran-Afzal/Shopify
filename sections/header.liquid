{%- liquid
  assign section_prefix_class = 'page-header'
  assign section_scheme_class = section.settings.section_color_scheme | prepend: 'scheme scheme--'
  assign menu_drawer_scheme_class = section.settings.menu_drawer_color_scheme | prepend: 'scheme scheme--'
  assign cart_view = settings.cart_view

  capture check_localization
    render 'check-localization-drops'
  endcapture

  capture local_exists
    if section.settings.enable_country_selector or section.settings.enable_language_selector
      if check_localization == 'true'
        echo 'true'
      endif
    endif
  endcapture

  capture enable_account_dashboard
    if shop.customer_accounts_enabled
      if customer
        echo 'true'
      endif
    endif
  endcapture

  capture action_lhs_exists
    if section.settings.show_store_locator or section.settings.show_customer
      echo 'true'
    endif
  endcapture

  capture action_rhs_exists
    if section.settings.show_cart or section.settings.enable_search
      echo 'true'
    endif
  endcapture

  capture sticky_behavior
    case section.settings.enable_sticky
      when 'on_scroll_up'
        echo 'upwards'
      when 'on_scroll'
        echo 'downwards'
      else
        echo 'stacked'
    endcase
  endcapture

  capture mega_menu_exists
    if section.blocks.size > 0
      for block in section.blocks
        if block.type == 'mega_menu'
          echo 'true'
          break
        endif
      endfor
      else
        echo 'false'
    endif
  endcapture

  capture predictive_tag
    if section.settings.enable_predictive_suggestions
      echo 'predictive-dropdown'
      else
        echo 'dropdown-component'
    endif
  endcapture

  capture media_height
  case section.settings.media_height
  when 'adapt'
      echo '--media-size-adapt'
    when 'small'
      echo '--small-height'
    when 'medium'
      echo '--medium-height'
  endcase
endcapture
-%}
