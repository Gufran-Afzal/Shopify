<div class="global-block {{ section_prefix_class }} {{ section_prefix_class }}-{{ section.id }} {% if section.settings.show_bottom_bdr %}--bdr-bottom{% endif %}" style ="--logo-desktop-width: {{ section.settings.logo_desktop_width }}px; --logo-mobile-width: {{ section.settings.logo_mobile_width }}px;">
  {%- unless section.settings.enable_sticky == 'none' -%}
    <sticky-block class="sticky-header-wrapper" data-ref-block=".page-header--fixed-area" data-body-class="page-header--fixed" data-revealed="false" data-behavior="{{ sticky_behavior }}" data-spacer="true">
  {%- endunless -%}
    <div class="{{ section_prefix_class }}--fixed-area">
      <div class="{{ section_scheme_class }}">
        <div class="container-fluid">
          <div class="{{ section_prefix_class }}_wrapper --v-align-middle">
            {%- if section.settings.menu.links != null -%}
              <nav class="{{ section_prefix_class }}_col {{ section_prefix_class }}_col--menu">
                <div class="{{ section_prefix_class }}_col--menu-block">
                  <modal-component-toggler>
                    <button
                      type="button"
                      data-expanded="false"
                      data-modal-ref="modal-mega-drawer"
                      aria-haspopup="dialog"
                      aria-controls="{{ section.id }}-modal-mega-drawer"
                      class="mobile-menu-opener mobile-mega-menu-opener --mega-dropper-icn"
                    >
                      <span class="mobile-menu_toggle" aria-hidden="true"></span>
                      <span class="button_icn-wrapper {{ section_prefix_class }}_burger-icn">
                        {%- render 'icons-data', width: 29, height: 29, use_icon: true, icon_reference: 'barlines', section_id: section.id -%}
                      </span>
                      <span class="{% unless section.settings.enable_desktop_drawer %}visually-hidden{% else %}mobile-menu_label h5 --fw-bold{% endunless %}">
                        {{- 'menu.burger_label' | t -}}
                      </span>
                    </button>
                  </modal-component-toggler>
                  <modal-component
                    data-modal="modal-menu-drawer"
                    data-container="body"
                  >
                    <div
                      class="modal modal-component --drawer modal-component--menu-drawer"
                      role="dialog"
                      aria-modal="true"
                      aria-labelledby="modal-menu-drawer-heading"
                      aria-describedby="{{ section.id }}-modal-menu-drawer"
                      tabindex="-1"
                    >
                      <div class="modal-component_wrapper menu-drawer-wrapper" id="{{ section.id }}-modal-menu-drawer">
                        <div class="modal-component_body">
                          <div class="modal-component_head --desktop-head">
                            <strong class="h6 modal-component_heading mty --fw-bold" id="modal-menu-drawer-heading">
                              {{- 'menu.burger_label' | t -}}
                            </strong>
                            <modal-component-toggler>
                              <button
                                type="button"
                                data-expanded="true"
                                data-toggle-attribute="false"
                                data-modal-ref="modal-menu-drawer"
                                class="button--icn button modal-component_btn-close"
                              >
                                <span class="button_icn-wrapper">
                                  {%- render 'icons-data', width: 14, height: 14, use_icon: true, icon_reference: 'cross' -%}
                                  <span class="visually-hidden">
                                    {{- 'accessibility.close' | t -}}
                                  </span>
                                </span>
                              </button>
                            </modal-component-toggler>
                          </div>
                          <div class="modal-component_content --top-menu-desktop">
                            <ul class="list-unstyled main-navigation --menu-flex --word-break {{ section.settings.links_font_weight }}"  style="--fw-menu-items: {{ section.settings.links_font_weight }};">
                              {%- for link in section.settings.menu.links -%}
                                <li class="main-navigation_item">
                                  <a href="{{- link.url -}}" class="main-navigation_link {% if link.current %}main-navigation_link--active{% endif %}" {% if link.current %}aria-current="{{- 'page.current_label' | t -}}"{% endif %}>
                                    {{- link.title -}}
                                  </a>
                                </li>
                              {%- endfor -%}
                            </ul>
                          </div>
                        </div>
                      </div>
                    </div>
                  </modal-component>
                </div>
                <div class="store-locatr--mob">
                  {%- if section.settings.show_store_locator -%}
                    <div class="icns-list_item {{ section_prefix_class }}_icns-items">
                      <modal-component-toggler class="{{ section_prefix_class }}_modal-toggler">
                        <button
                          type="button"
                          class="button--icn button"
                          data-expanded="false"
                          aria-haspopup="dialog"
                          data-modal-ref="modal-store-locator"
                          aria-controls="modal-store-locator"
                        >
                          <i class="button_icn-wrapper">
                            {%- render 'icons-data', width: 22, height: 22, use_icon: true, icon_reference: 'pin-area', section_id: section.id -%}
                            <span class="visually-hidden">
                              {{- 'store.toggler_label' | t -}}
                            </span>
                          </i>
                        </button>
                      </modal-component-toggler>
                    </div>
                  {%- endif -%}
                </div>
              </nav>
              {%- else -%}
                <div class="{{ section_prefix_class }}_col --empty {{ section_prefix_class }}_col--menu">&nbsp;</div>
            {%- endif -%}
            <div class="{{ section_prefix_class }}_col {{ section_prefix_class }}_col--actions">
              <div class="{{ section_prefix_class }}_box-wrap --v-align-middle">
                {%- if action_lhs_exists == 'true' -%}
                  <div class="{{ section_prefix_class }}_box-col {{ section_prefix_class }}_box-col--left">
                    <ul class="icns-list list-unstyled --v-align-middle">
                      {%- if section.settings.show_store_locator -%}
                        <li class="icns-list_item {{ section_prefix_class }}_icns-items">
                          <modal-component-toggler class="{{ section_prefix_class }}_modal-toggler">
                            <button
                              type="button"
                              class="button--icn button"
                              data-expanded="false"
                              aria-haspopup="dialog"
                              data-modal-ref="modal-store-locator"
                              aria-controls="modal-store-locator"
                            >
                              <i class="button_icn-wrapper">
                                {%- render 'icons-data', width: 22, height: 22, use_icon: true, icon_reference: 'pin-area', section_id: section.id -%}
                                <span class="visually-hidden">
                                  {{- 'store.toggler_label' | t -}}
                                </span>
                              </i>
                            </button>
                          </modal-component-toggler>
                        </li>
                      {%- endif -%}
                      {%- if section.settings.show_customer -%}
                        {%- if shop.customer_accounts_enabled -%}
                          {%- if enable_account_dashboard == 'true' -%}
                            <li class="icns-list_item {{ section_prefix_class }}_icns-items">
                              <a href="{{- routes.account_logout_url -}}" class="button button--icn">
                                <i class="button_icn-wrapper">
                                  {%- render 'icons-data', width: 22, height: 22, use_icon: true, icon_reference: 'user', section_id: section.id -%}
                                  <span class="visually-hidden">
                                    {{- 'customer.logout_label' | t -}}
                                  </span>
                                </i>
                              </a>
