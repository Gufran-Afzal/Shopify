<div class="global-block {{ section_prefix_class }} {{ section_prefix_class }}-{{ section.id }} {{ section_scheme_class }} --align-{{ section.settings.section_text_alignment }}" style="--section-padding-top: {{ section.settings.section_top_padding }}px; --section-padding-bottom: {{ section.settings.section_bottom_padding }}px;">
  <div class="container">
    <div class="{{ section_prefix_class }}_wrapper">
      {%- for block in section.blocks -%}
        {%- case block.type -%}
          {%- when '@app' -%}
            <div class="{{ section_prefix_class }}_row {{ section_prefix_class }}_row--{{ block.type }}" {{ block.shopify_attributes }}>
              {%- render block -%}
            </div>
          {%- when 'custom_liquid' -%}
            <div class="{{ section_prefix_class }}_row {{ section_prefix_class }}_row--{{ block.type }}" {{ block.shopify_attributes }}>
              {{- block.settings.custom_liquid -}}
            </div>
          {%- when 'featured_image' -%}
            {%- liquid 
              assign media_size = block.settings.section_media_size | default: block.settings.card_media_size

              capture card_media_size
                if media_size == 'adapt'
                  echo article.image.aspect_ratio | default: '1'
                else 
                  echo '1'
                endif
              endcapture
            -%}
            {%- unless article.image == blank -%}
              <div class="{{ section_prefix_class }}_row {{ section_prefix_class }}_row--{{ block.type }}" {{ block.shopify_attributes }}>
                <div class="{{ section_prefix_class }}_image-wrap --media-size-{{ media_size }}" style="--media-ratio: {{ card_media_size }};">
                  {%- liquid
                    capture image_alt
                      if article.image.alt != blank
                        echo article.image.alt
                        else
                          echo article.title | escape
                      endif
                    endcapture
                  -%}
                  {{- article.image | image_url: width: 2550 | image_tag: loading: loading_attr, class: 'object-fit object-fit--absolute', widths: '850, 1700', sizes: '(max-width: 767px) 850vw, 1700vw', alt: image_alt -}}
                </div>
              </div>
            {%- endunless -%}
          {%- when 'tags' -%}
            <div class="{{ section_prefix_class }}_row {{ section_prefix_class }}_row--{{ block.type }}" {{ block.shopify_attributes }}>
              <div class="{{ section_prefix_class }}_inline-wrap">
                {%- if article.tags.size > 0 and block.settings.show_tags -%}
                  <ul class="list-unstyled {{ section_prefix_class }}_tags-list {{ section_prefix_class }}_tags-list--scroll">
                    {%- for tag in article.tags -%}
                      <li class="{{ section_prefix_class }}_tag-item">
                        <strong class="{{ section_prefix_class }}_tag-link">
                          {{- tag -}}
                        </strong>
                      </li>
                    {%- endfor -%}
                  </ul>
                {%- endif -%}
              </div>
            </div>
          {%- when 'title' -%}
            <div class="{{ section_prefix_class }}_row {{ section_prefix_class }}_row--{{ block.type }}" {{ block.shopify_attributes }}>
              <h1 class="{{ block.settings.heading_size }} {{ block.settings.heading_weight }} mty {{ section_prefix_class }}_main-heading">{{- article.title -}}</h1>
            </div>
          {%- when 'meta' -%}
            <div class="{{ section_prefix_class }}_row {{ section_prefix_class }}_row--{{ block.type }}" {{ block.shopify_attributes }}>
              {%- if block.settings.show_author or block.settings.show_date or block.settings.show_comments_count -%}
                <div class="{{ section_prefix_class }}_stats-wrap">
                  {%- if block.settings.show_author -%}
                    <strong class="{{ section_prefix_class }}_subtitle {{ section_prefix_class }}_subtitle--author">
                      {{- article.author -}}
                    </strong>
                  {%- endif -%}
                  {%- if block.settings.show_date -%}
                    <strong class="{{ section_prefix_class }}_subtitle {{ section_prefix_class }}_subtitle--date">
                      {{- article.published_at | time_tag: format: 'date' -}}
                    </strong>
                  {%- endif -%}
                  {%- if block.settings.show_comments_count -%}
                    {%- if article.comments_count > 0 and blog.comments_enabled? -%}
                      <strong class="{{ section_prefix_class }}_subtitle {{ section_prefix_class }}_subtitle--comments-count">
                        {%- liquid
                          capture count
                            if article.comments_count > 1
                              echo 'blogs.comments.count.other' | t: count: article.comments_count
                              else
                                echo 'blogs.comments.count.one' | t: count: article.comments_count
                            endif
                          endcapture
                          echo count
                        -%}
                      </strong>
                    {%- endif -%}
                  {%- endif -%}
                </div>
              {%- endif -%}
            </div>
          {%- when 'content' -%}
            <div class="{{ section_prefix_class }}_row {{ section_prefix_class }}_row--{{ block.type }}" {{ block.shopify_attributes }}>
              {{- article.content -}}
            </div>
        {%- endcase -%}
      {%- endfor -%}
      {% if blog.next_article != blank or blog.previous_article != blank or section.settings.enable_share %}
        <nav class="{{ section_prefix_class }}_foot-nav {% if section.settings.enable_share %}--foot-nav{% endif %}">
          {%- if section.settings.enable_share -%}
            <div class="{{ section_prefix_class }}_share-link">
              {%- liquid
                assign share_url = request.origin | append: article.url
                render 'share-button', share_link: share_url, share_title: article.title, view: 'full-button', social_exists: social_exists, social_icons_enable: true, social_extra_class: '--q-align-justify', section_id: section.id
              -%}
            </div>
          {%- endif -%}
          {% if blog.next_article != blank or blog.previous_article != blank %}
            {%- if section.settings.enable_pagination -%}
              <div class="{{ section_prefix_class }}_foot-arrows">
                {%- if blog.next_article != blank -%}
                  <a class="{{ section_prefix_class }}_foot-link h6 --fw-medium {{ section_prefix_class }}_foot--link-prev" href="{{- blog.next_article.url -}}">
                    <div class="{{ section_prefix_class }}_prev-flex">
                      <span>{{ 'blogs.comments.previous' | t }}</span>
                      {%- render 'icons-data', width: 25, height: 15, use_icon: true, icon_reference: 'art-prev', section_id: section.id -%}
                    </div>
                    <div class="{{ section_prefix_class }}_next-img">
                      {%- if blog.next_article.image != blank -%}
                        {{- blog.next_article.image | image_url: width: 2550 | image_tag: loading: loading_attr, class: 'object-fit object-fit--absolute', widths: '850, 1700', sizes: '(max-width: 767px) 850vw, 1700vw', alt: image_alt -}}
                        {% else %}
                          {{- 'hero-apparel-1' | placeholder_svg_tag: 'object-fit object-fit--absolute' -}}
                      {%- endif -%}
                    </div>
                  </a>
                {%- endif -%}
                {%- if blog.previous_article != blank -%}
                  <a class="{{ section_prefix_class }}_foot-link h6 --fw-medium {{ section_prefix_class }}_foot--link-next" href="{{- blog.previous_article.url -}}">
                    <div class="{{ section_prefix_class }}_prev-img">
                      {%- if blog.previous_article.image != blank -%}
                        {{- blog.previous_article.image | image_url: width: 2550 | image_tag: loading: loading_attr, class: 'object-fit object-fit--absolute', widths: '850, 1700', sizes: '(max-width: 767px) 850vw, 1700vw', alt: image_alt -}}
                        {% else %}
                          {{- 'hero-apparel-1' | placeholder_svg_tag: 'object-fit object-fit--absolute' -}}
                      {%- endif -%}
                    </div>
                    <div class="{{ section_prefix_class }}_next-flex {% if section.settings.enable_share %}--left-auto{% else %}--right-auto{% endif %}">
                      <span>{{ 'blogs.comments.next' | t }}</span>
                      {%- render 'icons-data', width: 25, height: 15, use_icon: true, icon_reference: 'art-next', section_id: section.id -%}
                    </div>
                  </a>
                {%- endif -%}
              </div>
            {%- endif -%}
          {%- endif -%}
        </nav>
      {%- endif -%}
      {%- if blog.comments_enabled? -%}
        <aside class="{{ section_prefix_class }}_aside">
          <div id="comments" class="{{ section_prefix_class }}_comments-wrap">
            {%- if article.comments_count > 0 -%}
              <h2 class="mty h4 {{ section_prefix_class }}_comments-heading" id="Comments-{{ article.id }}">
                {%- liquid
                  capture count
                    if article.comments_count > 1
                      echo 'blogs.comments.count.other' | t: count: article.comments_count
                      else
                        echo 'blogs.comments.count.one' | t: count: article.comments_count
                    endif
                  endcapture
                  echo count
                -%}
              </h2>
            {%- endif -%}
            {%- paginate article.comments by 5 -%}
              <div class="{{ section_prefix_class }}_comments">
                {%- if comment.status == 'pending' and comment.content -%}
                  <div class="comment-row" id="{{- comment.id -}}">
                    <div class="comment-row_content text-size--large --word-break mty">
                      {{- comment.content -}}
                    </div>
                    <div class="comment-row_info">
                      <strong class="comment-row_text comment-row_text--author --word-break">
                        {{- comment.author -}}
                      </strong>
                    </div>
                  </div>
                {%- endif -%}
                {%- for comment in article.comments -%}
                  <div class="comment-row" id="{{- comment.id -}}">
                    <div class="comment-row_content text-size--large --word-break mty">
                      {{- comment.content -}}
                    </div>
                    <div class="comment-row_info text-size--small">

                      <strong class="comment-row_text comment-row_text--author --word-break">
                        {{- comment.author -}}
                      </strong>
                      <strong class="comment-row_text comment-row_text--date --word-break">
                        {{- comment.created_at | time_tag: format: 'date' -}}
                      </strong>
                    </div>
                  </div>
                {%- endfor -%}
              </div>
              {% render 'pagination', paginate: paginate, section_id: section.id %}
            {%- endpaginate -%}
            {% form 'new_comment', article %}
              {%- liquid
                capture post_message
                  if blog.moderated? and comment.status == 'unapproved'
                    echo 'blogs.comments.success_moderated' | t
                    else
                      echo 'blogs.comments.success' | t
                  endif
                endcapture
              -%}
              <h3 class="mty h4 {{ section_prefix_class }}_comment-heading">
                {{- 'blogs.comments.comment_form_title' | t -}}
              </h3>
              <div class="{{ section_prefix_class }}_comment-fields {% if blog.moderated? == false %}{{ section_prefix_class }}_comment-fields--moderated{% endif %}">
                <div class="{{ section_prefix_class }}_form-group">
                  <label class="visually-hidden" for="CommentForm-author">
                    {{- 'blogs.comments.name' | t -}}
                  </label>
                  <input
                    type="text"
                    name="comment[author]"
                    id="CommentForm-author"
                    autocomplete="name"
                    value="{{- form.author -}}"
                    title="{{- 'blogs.comments.name' | t -}}"
                    aria-required="true"
                    required
                    {% if form.errors contains 'author' %}
                      aria-invalid="true"
                      aria-describedby="CommentForm-author-error"
                    {% endif %}
                    placeholder="{{- 'blogs.comments.name' | t -}}*"
                  >
                  {%- if form.errors contains 'author'-%}
                    <div id="CommentForm-author-error" class="{{ section_prefix_class }}_error-wrapper" role="status">
                      <i class="{{ section_prefix_class }}_role-icn">
                        {%- render 'icons-data', width: 22, height: 22, use_icon: true, icon_reference: 'error' -%}
                      </i>
                      {{- 'blogs.comments.name' | t }} {{ form.errors.messages.author -}}
                    </div>
                  {%- endif -%}
                </div>
                <div class="{{ section_prefix_class }}_form-group">
                  <label class="visually-hidden" for="CommentForm-email">
                    {{- 'blogs.comments.email' | t -}}
                  </label>
                  <input
                    type="email"
                    name="comment[email]"
                    id="CommentForm-email"
                    autocomplete="email"
                    value="{{- form.email -}}"
                    title="{{- 'blogs.comments.email' | t -}}"
                    autocapitalize="off"
                    aria-required="true"
                    onkeydown="validation()"
                    required
                    {% if form.errors contains 'email' %}
                      aria-invalid="true"
                      aria-describedby="CommentForm-email-error"
                    {% endif %}
                    placeholder="{{- 'blogs.comments.email' | t -}}*"
                  >
                  <div class="{{ section_prefix_class }}_results-wrap results-wrap_message"></div>
                  {%- if form.errors contains 'email'-%}
                    <div id="CommentForm-email-error" class="{{ section_prefix_class }}_error-wrapper" role="status">
                      <i class="{{ section_prefix_class }}_role-icn">
                        {%- render 'icons-data', width: 22, height: 22, use_icon: true, icon_reference: 'error' -%}
                      </i>
                      {{- 'blogs.comments.email' | t }} {{ form.errors.messages.email -}}
                    </div>
                  {%- endif -%}
                </div>
                <div class="{{ section_prefix_class }}_form-group">
                  <label class="visually-hidden" for="CommentForm-body">
                    {{- 'blogs.comments.message' | t -}}
                  </label>
                  <textarea
                    rows="5"
                    name="comment[body]"
                    id="CommentForm-body"
                    aria-required="true"
                    title="{{- 'blogs.comments.name' | t -}}"
                    required
                    {% if form.errors contains 'body' %}
                      aria-invalid="true"
                      aria-describedby="CommentForm-body-error"
                    {% endif %}
                    placeholder="{{- 'blogs.comments.message' | t -}}"
                  >
                    {{- form.body -}}
                  </textarea>
                  <div class="{{ section_prefix_class }}_results-wrap results-wrap_message"></div>
                  {%- if form.errors contains 'body'-%}
                    <div id="CommentForm-body-error" class="{{ section_prefix_class }}_error-wrapper" role="status">
                      <i class="{{ section_prefix_class }}_role-icn">
                        {%- render 'icons-data', width: 22, height: 22, use_icon: true, icon_reference: 'error' -%}
                      </i>
                      {{- 'blogs.comments.message' | t }} {{ form.errors.messages.body -}}
                    </div>
                  {%- endif -%}
                </div>
              </div>
              {%- if blog.moderated? -%}
                <p class="article-template__comment-warning caption">
                  {{- 'blogs.comments.moderated' | t -}}
                </p>
              {%- endif -%}
              <div class="{{ section_prefix_class }}_form-btn-wrap">
                <button class="button button--primary" type="submit">
                  {{- 'blogs.comments.post' | t -}}
                </button>
              </div>
              {%- if form.errors -%}
                <strong class="form__message mty" role="alert" tabindex="-1" autofocus>
                  {%- render 'icons-data', width: 22, height: 22, use_icon: true, icon_reference: 'error' -%}
                  {{- 'blogs.comments.error.heading' | t -}}
                </strong>
                <ul class="errors-list list-unstyled">
                  {%- for field in form.errors -%}
                    <li class="errors-list_item">
                      {%- if form.errors.translated_fields[field] contains 'author' %}
                        {{- 'blogs.comments.error.name' | t }}
                        {%- elsif form.errors.translated_fields[field] contains 'body' %}
                          {{- 'blogs.comments.error.message' | t }}
                        {%- else %}
                          {{- form.errors.translated_fields[field] }}
                      {%- endif %}
                      {{ form.errors.messages[field] -}}
                    </li>
                  {%- endfor -%}
                </ul>
                {%- elsif form.posted_successfully? -%}
                  <div class="success-message success-message_wrapper" role="alert" tabindex="-1" autofocus>
                    <i class="success-message_icn">
                      {%- render 'icons-data', width: 54, height: 54, use_icon: true, icon_reference: 'smile' -%}
                    </i>
                    <p>{{- post_message -}}</p>
                  </div>
              {%- endif -%}
            {%- endform -%}
          </div>
        </aside>
      {%- endif -%}
    </div>
  </div>
</div>
<script type="application/ld+json">
  {{ article | structured_data }}
</script>
