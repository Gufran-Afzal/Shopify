{%- liquid
  assign section_prefix_class = 'main-article'
  assign section_scheme_class = section.settings.section_color_scheme | prepend: 'scheme scheme--'

  capture loading_attr
    if section.index > 1
      echo 'lazy'
      else
        echo 'eager'
    endif
  endcapture

  capture enable_social_list
    if settings.social_instagram_link != blank
      echo 'true'
      elsif settings.social_linkedin_link != blank
        echo 'true'
      elsif settings.social_facebook_link != blank
        echo 'true'
      elsif settings.social_youtube_link != blank
        echo 'true'
      elsif settings.social_twitter_link != blank
        echo 'true'
      elsif settings.social_snapchat_link != blank
        echo 'true'
      elsif settings.social_pinterest_link != blank
        echo 'true'
      elsif  settings.social_tiktok_link != blank
        echo 'true'
      elsif settings.social_tumblr_link != blank
        echo 'true'
      elsif settings.social_vimeo_link != blank
        echo 'true'
      else
        echo 'false'
    endif
  endcapture

  capture social_exists
    if enable_social_list == 'true' and section.settings.show_social_links
      echo 'true'
    endif
  endcapture
-%}
{%- style -%}
.breadcrumb-nav{
    padding-top: 0;
    padding-bottom: 0;
  }

  .breadcrumb-nav.--bdr-bottom{
    border-bottom: 1px solid var(--section-widget-border-color);
  }

  .breadcrumb_wrapper{
    padding-top: 21px;
    padding-bottom: 21px;
  }

  .breadcrumb-list,
  .breadcrumb-list_item{
    display: flex;
  }

  .breadcrumb-list{
    flex-wrap: wrap;
    width: 100%;
    font-size: 1.4rem;
    line-height: 2rem;
  }

  .breadcrumb-list_item + .breadcrumb-list_item::before{
    content: '';
    border-bottom: 1px solid var(--section-border-color);
    width: 2rem;
    display: inline-block;
    margin-left: 11px;
    margin-right: 11px;
  }

  .breadcrumb-list [aria-current="page"]{
    color: var(--heading-color);
    font-weight: 500;
  }

  .breadcrumb-list_link{
    text-decoration: none;
    transition: color 0.3s ease;
    font-size: 1.2rem;
    font-weight: 400;
  }

  .pagination-wrapper{
    padding-left: var(--cp-spacer-left, 15px);
    padding-right: var(--cp-spacer-right, 15px);
  }

  .pagination_list {
    flex-wrap: wrap;
    gap: 6px;
  }

  .pagination_next-pg{
    margin-right: 9.6px;
  }

  .pagination_back-pg{
    margin-left: 9.6px;
  }

  .pagination_item{
    border: 1px solid var(--section-border-color);
    border-radius: 4px;
  }

  .pagination_link {
    text-decoration: none;
    font-size: 1.4rem;
    font-weight: 400;
    line-height: 2rem;
    transition: color 0.35s ease;
    padding: 0.5rem 1rem;
    display: block;
  }

  a.pagination_link--active{
    background-color: var(--section-active-hover-color);
    color: var(--section-background-color);
  }

  .pagination-wrapper{
    margin-top: 47px;
  }

  .pagination-wrapper,
  .pagination,
  .pagination_list{
    display: flex;
    width: 100%;
    justify-content: center;
  }

  .load-more_button{
    display: flex;
    align-items: center;
    gap: 8px;
  }

  .load-more_loader{
    line-height: 1rem;
  }

  .form__message, 
  .error-email {
    display: flex;
  }

  .form-group{
    position: relative;
  }

  .form__message,
  .error-email {
    align-items: center;
    gap: 10px;
  }

  .form__message {
    font-weight: 400;
    font-size: 1.8rem;
    line-height: 2.8rem;
  }

  .newsletter-form_results-wrap,
  .form__message svg,
  .errors-list_link {
    color: var(--section-error-color);
  }

  .success-message .image,
  .errors-list {
    margin-bottom: 10px;
  }

  .errors-list_link {
    text-decoration: none;
  }

  .error-email {
    position: absolute;
    right: 0;
  }

  .error-email,
  .contact__field-error {
    font-size: 1.4rem;
    line-height: 2.1rem;
  }

  .success-message {
    text-align: center;
    margin-top: 39px;
    font-size: 2rem;
    line-height: 3.3rem;
    color: var(--section-success-color);
  }

  .success-message p {
    margin: 0;
  }

  .success-message_icn {
    display: block;
    margin-bottom: 14px;
  }

  .form-group textarea,
  .error-email{
    margin-top: 10px;
  }

  .form-wrapper_btn-wrap{
    margin-top: 19px;
  }

  .form-wrapper_button {
    width: 100%;
    --button-padding-y: 1.1rem;
    --button-padding-x: 1.6rem;
    --button-font-size: 1.4rem;
    --button-line-height: 2rem;
    --button-font-family: var(--body-font-family);
  }

  .share-button-wrap{
    margin-left: auto;
  }

  .share-button-wrap,
  .dropdown-component--share{
    position: relative;
  }

  .field__label,
  .share-button__copy,
  .share-button__close,
  .dropdown-component--share .field__label,
  .dropdown-component--share .share-button__message{
    position: absolute;
  }

  .dropdown-component_share-wrapper{
    width: 273px;
    right: -64px;
    left: auto;
    top: auto;
    bottom: 100%;
    transform: none;
    margin-bottom: 10px;
    padding: 40px 60px 10px 10px;
  }

  .--share-native .dropdown-component_share-wrapper{
    display: none;
  }

  .dropdown-component--share .field__label,
  .share-button__close{
    top: 5px;
  }

  .field__label{
    left: 23px;
  }

  .share-button__copy{
    right: 10px;
    bottom: 10px;
  }

  .share-button-wrap .form-group{
    width: calc(100% - 50px);
  }

  .share-button__close{
    right: 7px;
  }

  .dropdown-component--share .field__label,
  .share-button__message{
    top: -32px;
    left: 23px;
    z-index: 1;
  }

  .share-button__copy.button--icn{
    --button-padding-y: 1.4rem;
    --button-padding-x: 1.3rem;
  }

  .share-button__close.button--icn,
  .share-button__copy.button--icn{
    --btn-icn-bdr: transparent;
  }

  .share-button__close.button--icn{
    --button-padding-y: 0.5rem;
    --button-padding-x: 0.5rem;
  }

  .share-socials-list{
    display: flex;
    flex-wrap: wrap;
  }

  .share-socials-list--icns{
    line-height: 0;
    align-items: center;
    gap: 6px;
    justify-content: center;
  }

  .share-socials-list:empty{
    display: none;
  }

  .share-list-social_link{
    word-break: break-word;
    transition: color 0.3s ease, border-color 0.3s ease;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 4px;
    border: 1px solid transparent;
  }

  .share-list-social_text-link{
    text-decoration: none;
    font-size: 1.2rem;
    line-height: 1.8rem;
  }

  .main-article_inline-wrap,
  .main-article_tags-list,
  .main-article_foot-link,
  .main-article_error-wrapper,
  .main-article .socials-list--icns{
    gap: 10px;
  }

  .main-article_inline-wrap,
  .main-article_tags-list,
  .main-article_stats-wrap,
  .main-article_subtitle,
  .main-article_foot-link,
  .main-article_foot-arrows,
  .main-article_prev-flex,
  .main-article_next-flex,
  .comment-row_info,
  .comment-row_text,
  .main-article_error-wrapper,
  .--flex{
    display: flex;
  }

  .main-article_inline-wrap,
  .main-article_stats-wrap,
  .main-article_subtitle,
  .main-article_foot-link,
  .comment-row_info,
  .comment-row_text,
  .main-article_error-wrapper,
  .--flex{
    align-items: center;
  }

  .main-article_next-img,
  .main-article_prev-img{
    position: relative;
  }

  .main-article_next-img,
  .main-article_prev-img,
  .comment-row_text + .comment-row_text::before,
  .main-article_subtitle + .main-article_subtitle::before,
  .main-article_error-wrapper{
    line-height: 0;
  }

  .main-article_next-img,
  .main-article_prev-img{
    position: relative;
    width: 50px;
    height: 50px;
  }

  .main-article_tags-list{
    overflow-x: auto;
    overflow-y: hidden;
    padding-top: 2px;
    padding-bottom: 2px;
  }

  .main-article_row--content{
    overflow: hidden;
  }

  .main-article_row--featured_image{
    margin-bottom: 30px;
  }

  .main-article_row--meta,
  .main-article_foot-link:focus-visible{
    margin-bottom: 2px;
  }

  .main-article_row--title{
    margin-bottom: 21px;
  }

  .main-article_row--tags{
    margin-bottom: 19px;
  }

  .main-article_row--content{
    margin-bottom: 26px;
  }

  .main-article_main-heading:hover{
    color: var(--section-active-hover-color);
  }

  .main-article_stats-wrap,
  .comment-row_info{
    flex-wrap: wrap;
  }

  .main-article_subtitle,
  .comment-row_text,
  .main-article_tag-link{
    font-weight: 400;
  }

  .main-article_foot-link .ico-left-arrow,
  .main-article_foot-link .ico-right-arrow,
  .comment-row_text + .comment-row_text::before,
  .main-article_subtitle + .main-article_subtitle::before,
  .share-text,
  .main-article_prev-flex,
  .main-article_next-flex,
  .main-article_tag-item,
  .main-article .share-button__text{
    flex-shrink: 0;
  }

  .comment-row_text + .comment-row_text::before,
  .main-article_subtitle + .main-article_subtitle::before{
    content: "-";
    background: var(--section-foreground-color);
    pointer-events: none;
    margin-left: 3px;
    margin-right: 3px;
  }

  .main-article_foot-link{
    font-size: 1.2rem;
    line-height: 2rem;
    transition: color 0.3s ease;
    margin: 0;
  }

  .main-article_foot-link:focus-visible{
    padding: 2px;
  }

  .main-article_share-link{
    margin-bottom: 14px;
  }

  .share-text,
  .main-article_tag-link{
    display: block;
  }

  .share-text{
    text-align: center;
  }

  .main-article_foot-nav{
    padding-top: 21px;
    border: solid var(--section-border-color);
    border-width: 0 0 1px 0;
  }

  .main-article_foot-arrows{
    gap: 20px;
  }

  .main-article_foot--link-next,
  .main-article_prev-flex svg,
  .--left-auto svg{
    margin-left: auto;
  }

  .main-article_foot--link-prev,
  .--right-auto svg{
    margin-right: auto;
  }

  .main-article_prev-flex,
  .main-article_next-flex{
    flex-direction: column;
  }

  .main-article_form-btn-wrap,
  .main-article_form-group + .main-article_form-group,
  .comment-row + .comment-row{
    margin-top: 20px;
  }

  .main-article_form-btn-wrap button{
    width: 100%;
  }

  .main-article_aside,
  .comment-form{
    margin-top: 41px;
  }

  .main-article_comment-heading{
    margin-bottom: 20px;
  }

  .comment-row{
    background-color: var(--section-foreground-color);
    padding: 12px 22px;
    border-radius: var(--input-corner-radius);
    color: var(--section-background-color);
  }

  .comment-row_info,
  .main-article_error-wrapper{
    justify-content: flex-end;
  }

  .main-article_comments-heading{
    --mt-bottom: 20px;
  }

  .main-article_tag-link{
    padding: 7px 13px;
    border-radius: calc(var(--section-radius) + 100px);
    background-color: var(--section-widget-background-color);
    color: var(--section-widget-foreground-color);
    transition: color 0.3s ease, background-color 0.3s ease, border-color 0.3s ease;
  }

  .main-article_tag-link,
  .main-article_stats-wrap,
  .main-article_error-wrapper,
  .main-article .share-button__text {
    font-size: 1.4rem;
    line-height: 1.8rem;
  }

  .comment-row_content{
    --mt-bottom: 24px;
  }

  .main-article_error-wrapper{
    margin-top: 10px;
    color: var(--section-error-color)
  }

  .main-article_foot-link{
    text-decoration: none;
  }

  .main-article .share-button__button {
    --button-background: transparent;
    --button-border-color: transparent;
    --button-text: var(--section-active-hover-color);
    --button-hover-background: transparent;
    --button-hover-border-color: transparent;
    --button-hover-text: var(--section-links-color);
    --button-padding-x: 0;
  }

  @media (min-width: 576px){
    .dropdown-component_share-wrapper{
      width: 350px;
      right: 0;
    }

    .main-article .socials-list--icns{
      justify-content: flex-start;
    }

    .share-text{
      text-align: unset;
    }

    .main-article_foot-link{
      font-size: 1.4rem;
    }
  }

  @media (min-width: 768px){
    .dropdown-component_share-wrapper{
      width: 430px;
    }

    .share-socials-list--icns{
      gap: 21px;
    }

    .main-article_foot-link,
    .main-article .share-button__text{
      font-size: 1.8rem;
      line-height: 3rem;
    }

    .main-article_foot-link{
      gap: 21px;
    }

    .main-article_next-img,
    .main-article_prev-img{
      width: 79px;
      height: 81px;
    }
  }

  @media(min-width: 1024px){
    .--foot-nav{
      display: flex;
      align-items: center;
      justify-content: space-between;
      gap: 20px;
    }

    .main-article_share-link{
      justify-content: flex-start;
      margin-bottom: 0;
    }	
  }

{%- endstyle -%}
{%- liquid
  if section.settings.show_breadcrumb
    render 'breadcrumb', border_top: section.settings.show_top_bdr, border_bottom: section.settings.show_bottom_bdr, color_scheme: section.settings.breadcrumb_color_scheme
  endif
-%}
