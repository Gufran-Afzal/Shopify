<div class="global-block {{ section_prefix_class }} {{ section_prefix_class }}-{{ section.id }} {{ section_scheme_class }} grid-common" id="{{- section.id -}}" style="--section-padding-top: {{ section.settings.section_top_padding }}px; --section-padding-bottom: {{ section.settings.section_bottom_padding }}px; --columns-count: {{ section.settings.number_of_columns_on_desktop }}; --mobile-columns-count: {{ section.settings.number_of_columns_on_mobile }}; --mobile-q-align: {{ section.settings.section_mobile_text_alignment }}; --desktop-q-align: {{ section.settings.section_text_alignment }};">
  <div class="container">
    <div class="{{ section_prefix_class }}_wrapper grid-common_wrapper">
      {%- if section.settings.section_heading != blank or section.settings.section_caption  != blank -%}
        <header class="heading-head inner-container --word-break --q-align {% if section.settings.thumbnail_layout == 'slider' %}{% if section.settings.show_pagination %}--header-padding{% endif %}{% endif %}" {% unless section.settings.show_pagination %}style="--max-content: {{ max_header_size }}px;"{% else %}style="--max-content: {{ max_header_size }}px;"{% endunless %}>
          {%- if section.settings.section_caption != blank -%}
            <div class="{{ section_prefix_class }}_description --word-break {{ section.settings.section_caption_size }} {{ section.settings.section_caption_weight }} mty">
              {{- section.settings.section_caption -}}
            </div>
          {%- endif -%}
          {%- if section.settings.section_heading != blank -%}
            <h2 class="{{ section_prefix_class }}_heading --h-anchor-none {{ section.settings.section_heading_size }} {{ section.settings.section_heading_weight }} mty">
              {{- section.settings.section_heading -}}
            </h2>
          {%- endif -%}
        </header>
      {%- endif -%}
      <div class="grid-common_area-wrap">
        {%- if section.settings.thumbnail_layout == 'slider' -%}
          <splide-slider
            data-slider-type="default"
          >
            <div class="splide {{ section_prefix_class }}_slider"
              data-splide='{
                "drag": true,
                "autoplay": {{- section.settings.auto_play -}},
                "rewind": true,
                "gap": "-1px",
                "perMove": 1,
                "perPage": 1,
                "arrows": {{- section.settings.show_navigation -}},
                "pagination": {{- section.settings.show_pagination -}},
                "interval": {{- section.settings.change_slides_every | times: 1000 -}},
                "mediaQuery": "min",
                  "breakpoints": {
                    "380": {
                      "perPage": {{ section.settings.number_of_columns_on_mobile }}
                    },
                    "576": {
                      "perPage": 2
                    },
                    "768": {
                      "perPage": 3
                    },
                    "992": {
                      "perPage": {{ section.settings.number_of_columns_on_desktop }}
                    }
                  },
                "type": "slide"
              }'
            >
              <div class="splide__track">
                <div class="splide__list splide-slider--h-scroll" style="--init-divider: {{ section.settings.number_of_columns_on_desktop }}; --init-mob-divider: {{ section.settings.number_of_columns_on_mobile }};">
                  {%- if section.settings.section_collection != blank and section.settings.section_collection.all_products_count > 0 -%}
                    {%- for product in section.settings.section_collection.products limit: section.settings.products_to_show -%}
                      <div class="splide__slide --slide">
                        {%- liquid
                          capture loading_attr
                            unless section.index != 1
                              unless forloop.first
                                echo 'lazy'
                                else
                                  echo 'eager'
                              endunless
                              else
                                echo 'lazy'
                            endunless
                          endcapture

                          render 'product-column', product: product, heading_tag: 'h2', columns_count: section.settings.number_of_columns_on_desktop, media_size: section.settings.product_media_size, enable_atc: section.settings.enable_atc, show_swatches: section.settings.show_swatches, show_price: section.settings.show_price, show_vendor: section.settings.show_vendor, show_stock_tag: section.settings.show_stock_tag, show_sale_tag: section.settings.show_sale_tag, heading_size: section.settings.product_heading_size, heading_weight: section.settings.product_heading_weight, btns_position: section.settings.btns_position, show_quick_view: section.settings.enable_quick_view, loading_attr: loading_attr, section_id: section.id
                        -%}
                      </div>
                    {%- endfor -%}
                    {%- else -%}
                      {%- for placeholder_block in (1..section.settings.products_to_show) -%}
                        <div class="splide__slide">
                          {%- liquid
                            render 'product-column-placeholder', product: product, heading_tag: 'h2', columns_count: section.settings.number_of_columns_on_desktop, media_size: section.settings.product_media_size, enable_atc: section.settings.enable_atc, show_swatches: section.settings.show_swatches, show_price: section.settings.show_price, show_vendor: section.settings.show_vendor, show_stock_tag: section.settings.show_stock_tag, heading_size: section.settings.product_heading_size, heading_weight: section.settings.product_heading_weight, btns_position: section.settings.btns_position, show_quick_view: section.settings.enable_quick_view, loading_attr: loading_attr, section_id: section.id
                          -%}
                        </div>
                      {%- endfor -%}
                  {%- endif -%}
                </div>
              </div>
              {%- if section.settings.show_navigation or section.settings.show_pagination -%}
                <div class="splide-controls splide__arrows --justify-end">
                  {%- if section.settings.show_navigation -%}
                    <button class="splide__arrow splide__arrow--prev button button--primary button--icn">
                      <span class="visually-hidden">{{- 'accessibility.slider.previous' | t -}}</span>
                      {%- render 'icons-data', width: 10, height: 10, use_icon: true, icon_reference: 'left-arrow' -%}
                    </button>
                  {%- endif -%}
                  <ul
                    class="splide__pagination {% if section.settings.show_navigation %}--padding{% endif %}"
                    role="tablist"
                    aria-label="{{- 'accessibility.slider.pagination_label' | t -}}"
                  ></ul>
                  {%- if section.settings.show_navigation -%}
                    <button class="splide__arrow splide__arrow--next button button--primary button--icn">
                      <span class="visually-hidden">{{- 'accessibility.slider.next' | t -}}</span>
                      {%- render 'icons-data', width: 10, height: 10, use_icon: true, icon_reference: 'right-arrow' -%}
                    </button>
                  {%- endif -%}
                </div>
              {%- endif -%}
            </div>
          </splide-slider>
        {%- else -%}
          <div class="grid-common_cols-wrap grid grid--no-gutters {{ section_prefix_class }}_cols-wrap">
            {%- if section.settings.section_collection != blank and section.settings.section_collection.all_products_count != 0 -%}
              {%- for product in section.settings.section_collection.products limit: section.settings.products_to_show -%}
                <div class="grid_col">
                  {%- liquid
                    capture loading_attr
                      unless section.index != 1
                        unless forloop.first
                          echo 'lazy'
                          else
                            echo 'eager'
                        endunless
                        else
                          echo 'lazy'
                      endunless
                    endcapture
                    
                    render 'product-column', product: product, heading_tag: 'h2', columns_count: section.settings.number_of_columns_on_desktop, media_size: section.settings.product_media_size, enable_atc: section.settings.enable_atc, show_swatches: section.settings.show_swatches, show_price: section.settings.show_price, show_vendor: section.settings.show_vendor, show_stock_tag: section.settings.show_stock_tag, show_sale_tag: section.settings.show_sale_tag, heading_size: section.settings.product_heading_size, heading_weight: section.settings.product_heading_weight, btns_position: section.settings.btns_position, show_quick_view: section.settings.enable_quick_view, loading_attr: loading_attr, section_id: section.id
                  -%}
                </div>
              {%- endfor -%}
              {%- else -%}
                {%- for placeholder_block in (1..section.settings.products_to_show) -%}
                  <div class="grid_col">
                    {%- liquid
                      render 'product-column-placeholder', product: product, heading_tag: 'h2', columns_count: section.settings.number_of_columns_on_desktop, media_size: section.settings.product_media_size, enable_atc: section.settings.enable_atc, show_swatches: section.settings.show_swatches, show_price: section.settings.show_price, show_vendor: section.settings.show_vendor, show_stock_tag: section.settings.show_stock_tag, heading_size: section.settings.product_heading_size, heading_weight: section.settings.product_heading_weight, btns_position: section.settings.btns_position, show_quick_view: section.settings.enable_quick_view, loading_attr: loading_attr, section_id: section.id, section_id: section.id
                    -%}
                  </div>
                {%- endfor -%}
            {%- endif -%}
          </div>
        {%- endif -%}
      </div>
      {%- if section.settings.enable_view_more -%}
        {%- liquid
          capture show_more_button
            if section.settings.section_collection.all_products_count > section.settings.products_to_show
              echo 'true'
              else
                echo 'false'
            endif
          endcapture

          capture button_label
            if section.settings.view_more_label != blank
              echo section.settings.view_more_label
              else
                echo 'collection.view_all' | t
            endif
          endcapture
        -%}
        {%- if show_more_button == 'true' -%}
          <a
            href="{{- section.settings.section_collection.url -}}"
            class="button button--primary {{ section_prefix_class }}_view-btn"
          >
            {{- button_label -}}
          </a>
        {%- endif -%}
      {%- endif -%}
    </div>
  </div>
</div>
